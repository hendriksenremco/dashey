{
  "extends": [
    "@nuxtjs/eslint-config-typescript"
  ],
  "rules": {
    "arrow-parens": [
      "error",
      "as-needed"
    ],
    "vue/multi-word-component-names": 0,
    "vue/html-closing-bracket-newline": [
      "error",
      {
        "singleline": "never",
        "multiline": "never"
      }
    ],
  }
}